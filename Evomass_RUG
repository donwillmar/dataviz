############################################
####ui.R####
ui <- fluidPage(
    theme = shinythemes::shinytheme("superhero"),
    titlePanel("Mass Evolution of Rugby Internationals"),
    sidebarPanel(position = "left",
                 wellPanel(h3("Variables"),
                           radioButtons(inputId = "yvar", label = h4("Mass Metric"), 
                                        choices = list("Body Mass Index" = "BMI", "Weight (KG)" = "KG","Height (CM)" = "CM"),
                                        selected = "BMI"),
                           
                           selectInput(inputId = "zone", label = h4("Position Zone"), 
                                       choices = list("Front Row", "Second Row", "Back Row",
                                                      "Scrumhalf", "Flyhalf", "Centre", "Outside Back"),
                                       selected = "Front Row"),
                           
                           selectInput(inputId = "comp", label = h4("Compare Position"), 
                                       choices = list("None", "Front Row", "Second Row", "Back Row",
                                                      "Scrumhalf", "Flyhalf", "Centre", "Outside Back"),
                                       selected = "None")
                 ),
                 wellPanel(position = "left",
                           h4("Note:"),
                           helpText("Data collected from", strong("ESPN Scrum's"),
                                    a("Statguru.", 
                                      href = "http://stats.espnscrum.com/statsguru/rugby/stats/index.html?class=1;filter=advanced;orderby=matches;size=200;template=results;type=player"),
                                    "Data is limited prior to 1987."),
                           helpText("Find me on", a("Twitter",href = "https://twitter.com/donwillmar"), "or on", a("Instagram", href = "https://www.instagram.com/sportviz_data/")
                           )
                 )
    ),
    mainPanel(
        plotOutput("graf")
    )
)

####server.R####
server <- function(input, output) {
    
    selectedData <- reactive({
        c(input$yvar, input$zone, input$comp)
    })
    
    output$graf <- renderPlot ({
        
#### IF//ELSE for Comparison Positions: BMI####
    
            if ((input$yvar == "BMI") & (input$zone == "Front Row") & (input$comp == "None")){  
            plots[[1]]
        } else if ((input$yvar == "BMI") & (input$zone == "Front Row") & (input$comp == "Front Row")){  
            plots[[1]]
        } else if ((input$yvar == "BMI") & (input$zone == "Front Row") & (input$comp == "Second Row")) {
            plots[[28]]
        } else if ((input$yvar == "BMI") & (input$zone == "Front Row") & (input$comp == "Back Row")) {
            plots[[29]]
        } else if ((input$yvar == "BMI") & (input$zone == "Front Row") & (input$comp == "Scrumhalf")) {
            plots[[30]]
        } else if ((input$yvar == "BMI") & (input$zone == "Front Row") & (input$comp == "Flyhalf")) {
            plots[[31]]
        } else if ((input$yvar == "BMI") & (input$zone == "Front Row") & (input$comp == "Centre")) {
            plots[[32]]
        } else if ((input$yvar == "BMI") & (input$zone == "Front Row") & (input$comp == "Outside Back")) {
            plots[[33]]
        
        } else if ((input$yvar == "BMI") & (input$zone == "Second Row") & (input$comp == "None")){ 
            plots[[2]]
        } else if ((input$yvar == "BMI") & (input$zone == "Second Row") & (input$comp == "Second Row")){ 
            plots[[2]]
        } else if ((input$yvar == "BMI") & (input$zone == "Second Row") & (input$comp == "Front Row")) {
            plots[[28]]
        } else if ((input$yvar == "BMI") & (input$zone == "Second Row") & (input$comp == "Back Row")) {
            plots[[34]]
        } else if ((input$yvar == "BMI") & (input$zone == "Second Row") & (input$comp == "Scrumhalf")) {
            plots[[35]]
        } else if ((input$yvar == "BMI") & (input$zone == "Second Row") & (input$comp == "Flyhalf")) {
            plots[[36]]
        } else if ((input$yvar == "BMI") & (input$zone == "Second Row") & (input$comp == "Centre")) {
            plots[[37]]
        } else if ((input$yvar == "BMI") & (input$zone == "Second Row") & (input$comp == "Outside Back")) {
            plots[[38]]
        
        } else if ((input$yvar == "BMI") & (input$zone == "Back Row") & (input$comp == "None")){ 
            plots[[3]]
        } else if ((input$yvar == "BMI") & (input$zone == "Back Row") & (input$comp == "Back Row")){ 
            plots[[3]]
        } else if ((input$yvar == "BMI") & (input$zone == "Back Row") & (input$comp == "Front Row")) {
            plots[[29]]
        } else if ((input$yvar == "BMI") & (input$zone == "Back Row") & (input$comp == "Second Row")) {
            plots[[34]]
        } else if ((input$yvar == "BMI") & (input$zone == "Back Row") & (input$comp == "Scrumhalf")) {
            plots[[39]]
        } else if ((input$yvar == "BMI") & (input$zone == "Back Row") & (input$comp == "Flyhalf")) {
            plots[[40]]
        } else if ((input$yvar == "BMI") & (input$zone == "Back Row") & (input$comp == "Centre")) {
            plots[[41]]
        } else if ((input$yvar == "BMI") & (input$zone == "Back Row") & (input$comp == "Outside Back")) {
            plots[[42]]
        
        } else if ((input$yvar == "BMI") & (input$zone == "Scrumhalf") & (input$comp == "None")){ 
            plots[[4]]
        } else if ((input$yvar == "BMI") & (input$zone == "Scrumhalf") & (input$comp == "Scrumhalf")){ 
            plots[[4]]
        } else if ((input$yvar == "BMI") & (input$zone == "Scrumhalf") & (input$comp == "Front Row")) {
            plots[[30]]
        } else if ((input$yvar == "BMI") & (input$zone == "Scrumhalf") & (input$comp == "Second Row")) {
            plots[[35]] 
        } else if ((input$yvar == "BMI") & (input$zone == "Scrumhalf") & (input$comp == "Back Row")) {
            plots[[39]]
        } else if ((input$yvar == "BMI") & (input$zone == "Scrumhalf") & (input$comp == "Flyhalf")) {
            plots[[43]]
        } else if ((input$yvar == "BMI") & (input$zone == "Scrumhalf") & (input$comp == "Centre")) {
            plots[[44]]
        } else if ((input$yvar == "BMI") & (input$zone == "Scrumhalf") & (input$comp == "Outside Back")) {
            plots[[45]]

        } else if ((input$yvar == "BMI") & (input$zone == "Flyhalf") & (input$comp == "None")){ 
            plots[[5]]
        } else if ((input$yvar == "BMI") & (input$zone == "Flyhalf") & (input$comp == "Flyhalf")){ 
            plots[[5]]
        } else if ((input$yvar == "BMI") & (input$zone == "Flyhalf") & (input$comp == "Front Row")) {
            plots[[31]]
        } else if ((input$yvar == "BMI") & (input$zone == "Flyhalf") & (input$comp == "Second Row")) {
            plots[[36]]
        } else if ((input$yvar == "BMI") & (input$zone == "Flyhalf") & (input$comp == "Back Row")) {
            plots[[40]]
        } else if ((input$yvar == "BMI") & (input$zone == "Flyhalf") & (input$comp == "Scrumhalf")) {
            plots[[43]]
        } else if ((input$yvar == "BMI") & (input$zone == "Flyhalf") & (input$comp == "Centre")) {
            plots[[46]]
        } else if ((input$yvar == "BMI") & (input$zone == "Flyhalf") & (input$comp == "Outside Back")) {
            plots[[47]]

        } else if ((input$yvar == "BMI") & (input$zone == "Centre") & (input$comp == "None")){ 
            plots[[6]]
        } else if ((input$yvar == "BMI") & (input$zone == "Centre") & (input$comp == "Centre")){ 
            plots[[6]]
        } else if ((input$yvar == "BMI") & (input$zone == "Centre") & (input$comp == "Front Row")) {
            plots[[32]]
        } else if ((input$yvar == "BMI") & (input$zone == "Centre") & (input$comp == "Second Row")) {
            plots[[37]]
        } else if ((input$yvar == "BMI") & (input$zone == "Centre") & (input$comp == "Back Row")) {
            plots[[41]]
        } else if ((input$yvar == "BMI") & (input$zone == "Centre") & (input$comp == "Scrumhalf")) {
            plots[[44]]
        } else if ((input$yvar == "BMI") & (input$zone == "Centre") & (input$comp == "Flyhalf")) {
            plots[[46]]
        } else if ((input$yvar == "BMI") & (input$zone == "Centre") & (input$comp == "Outside Back")) {
            plots[[48]]
            
        } else if ((input$yvar == "BMI") & (input$zone == "Outside Back") & (input$comp == "None")){ 
            plots[[7]]
        } else if ((input$yvar == "BMI") & (input$zone == "Outside Back") & (input$comp == "Outside Back")){ 
            plots[[7]]
        } else if ((input$yvar == "BMI") & (input$zone == "Outside Back") & (input$comp == "Front Row")) {
            plots[[33]]
        } else if ((input$yvar == "BMI") & (input$zone == "Outside Back") & (input$comp == "Second Row")) {
            plots[[38]]
        } else if ((input$yvar == "BMI") & (input$zone == "Outside Back") & (input$comp == "Back Row")) {
            plots[[42]]
        } else if ((input$yvar == "BMI") & (input$zone == "Outside Back") & (input$comp == "Scrumhalf")) {
            plots[[45]]
