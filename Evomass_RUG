    geom_point(data=dfRUG[which(dfRUG$Role=="Centre"),], aes(x=Debut, y=CM),colour="purple", size=3, alpha=0.5)+
    geom_smooth(data=dfRUG[which(dfRUG$Role=="Second Row"),], aes(x=Debut, y=CM),se=F, colour="darkorange3")+
    geom_smooth(data=dfRUG[which(dfRUG$Role=="Centre"),], aes(x=Debut, y=CM),se=F, colour="magenta")+
