    geom_smooth(data=dfRUG[which(dfRUG$Role=="Centre"),], aes(x=Debut, y=CM),se=F, colour="magenta")+
    theme_wsj()+theme(axis.title=element_text(size=12))+
    labs(title = "Mass Evolution of Rugby Internationals", x="Year of Debut", y="Height (CM)",
         subtitle = "Position: Forwards vs Centre")

