############################################
####PACKAGES####

library(shiny)
library(ggplot2)
library(rio)

####IMPORT####

dfRUG<-import("Data/evoRUG.xlsx")

############################################
####BMI: Front Row Comparisons [28-33] ####

gg1 <- ggplot()+
    geom_point(data=dfRUG, aes(x=Debut, y=BMI),colour="slategray", size=3, alpha=0.5)+
    geom_point(data=dfRUG[which(dfRUG$Role=="Front Row"),], aes(x=Debut, y=BMI),colour="red", size=3, alpha=0.5)+
    geom_smooth(data=dfRUG[which(dfRUG$Role=="Front Row"),], aes(x=Debut, y=BMI),se=F, colour="black")+
    theme_wsj()+theme(axis.title=element_text(size=12))+
    labs(title = "Mass Evolution of Rugby Internationals", x="Year of Debut", y="Body Mass Index",
         subtitle = "Position: Front Row")

gg28 <- ggplot()+
    geom_point(data=dfRUG, aes(x=Debut, y=BMI),colour="slategray", size=3, alpha=0.5)+
    geom_point(data=dfRUG[which(dfRUG$Role=="Front Row"),], aes(x=Debut, y=BMI),colour="red", size=3, alpha=0.5)+
    geom_point(data=dfRUG[which(dfRUG$Role=="Second Row"),], aes(x=Debut, y=BMI),colour="orange", size=3, alpha=0.5)+
    geom_smooth(data=dfRUG[which(dfRUG$Role=="Front Row"),], aes(x=Debut, y=BMI),se=F, colour="firebrick")+
    geom_smooth(data=dfRUG[which(dfRUG$Role=="Second Row"),], aes(x=Debut, y=BMI),se=F, colour="darkorange3")+
    geom_smooth(data=dfRUG, aes(x=Debut, y=BMI),se=F, colour="slategray2")+
    theme_wsj()+theme(axis.title=element_text(size=12))+
    labs(title = "Mass Evolution of Rugby Internationals", x="Year of Debut", y="Body Mass Index",
         subtitle = "Positions: Front Row vs Second Row")

gg29 <- ggplot()+
    geom_point(data=dfRUG, aes(x=Debut, y=BMI),colour="slategray", size=3, alpha=0.5)+
    geom_point(data=dfRUG[which(dfRUG$Role=="Front Row"),], aes(x=Debut, y=BMI),colour="red", size=3, alpha=0.5)+
    geom_point(data=dfRUG[which(dfRUG$Role=="Back Row"),], aes(x=Debut, y=BMI),colour="gold", size=3, alpha=0.5)+
    geom_smooth(data=dfRUG[which(dfRUG$Role=="Front Row"),], aes(x=Debut, y=BMI),se=F, colour="firebrick")+
    geom_smooth(data=dfRUG[which(dfRUG$Role=="Back Row"),], aes(x=Debut, y=BMI),se=F, colour="yellow")+
    geom_smooth(data=dfRUG, aes(x=Debut, y=BMI),se=F, colour="slategray2")+
    theme_wsj()+theme(axis.title=element_text(size=12))+
    labs(title = "Mass Evolution of Rugby Internationals", x="Year of Debut", y="Body Mass Index",
         subtitle = "Positions: Front Row vs Back Row")

gg30 <- ggplot()+
    geom_point(data=dfRUG, aes(x=Debut, y=BMI),colour="slategray", size=3, alpha=0.5)+
    geom_point(data=dfRUG[which(dfRUG$Role=="Front Row"),], aes(x=Debut, y=BMI),colour="red", size=3, alpha=0.5)+
    geom_point(data=dfRUG[which(dfRUG$Role=="Scrumhalf"),], aes(x=Debut, y=BMI),colour="green", size=3, alpha=0.5)+
    geom_smooth(data=dfRUG[which(dfRUG$Role=="Front Row"),], aes(x=Debut, y=BMI),se=F, colour="firebrick")+
    geom_smooth(data=dfRUG[which(dfRUG$Role=="Scrumhalf"),], aes(x=Debut, y=BMI),se=F, colour="lawngreen")+
    geom_smooth(data=dfRUG, aes(x=Debut, y=BMI),se=F, colour="slategray2")+
    theme_wsj()+theme(axis.title=element_text(size=12))+
    labs(title = "Mass Evolution of Rugby Internationals", x="Year of Debut", y="Body Mass Index",
         subtitle = "Positions: Front Row vs Scrumhalf")

gg31 <- ggplot()+
    geom_point(data=dfRUG, aes(x=Debut, y=BMI),colour="slategray", size=3, alpha=0.5)+
    geom_point(data=dfRUG[which(dfRUG$Role=="Front Row"),], aes(x=Debut, y=BMI),colour="red", size=3, alpha=0.5)+
